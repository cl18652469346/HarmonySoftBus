{
  "app": {
    "bundleName": "com.hoperun.control",
    "vendor": "hoperun",
    "version": {
      "code": 1000000,
      "name": "1.0.0"
    }
  },
  "deviceConfig": {},
  "module": {
    "package": "com.hoperun.control",
    "name": ".MyApplication",
    "mainAbility": "com.hoperun.control.MainAbility",
    "deviceType": [
      "phone",
      "tablet"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry",
      "installationFree": false
    },
    "abilities": [
      {
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ],
        "orientation": "unspecified",
        "name": "com.hoperun.control.MainAbility",
        "icon": "$media:icon",
        "description": "$string:mainability_description",
        "label": "$string:entry_MainAbility",
        "type": "page",
        "launchType": "standard",
        "metaData": {
          "customizeData": [
            {
              "name": "hwc-theme",
              "extra": "",
              "value": "androidhwext:style/Theme.Emui.Light.NoTitleBar"
            }
          ]
        }
      },
      {
        "orientation": "unspecified",
        "name": "com.hoperun.control.RemoteControlAbility",
        "icon": "$media:icon",
        "description": "$string:testsoftability_description",
        "label": "$string:entry_TestSoftAbility",
        "type": "page",
        "launchType": "standard",
        "metaData": {
          "customizeData": [
            {
              "name": "hwc-theme",
              "extra": "",
              "value": "androidhwext:style/Theme.Emui.Light.NoTitleBar"
            }
          ]
        }
      },
      {
        "name": "com.hoperun.control.RemoteService",
        "icon": "$media:icon",
        "description": "$string:remoteservice_description",
        "type": "service"
      }
    ],
    "reqPermissions": [
      {
        "name": "ohos.permission.DISTRIBUTED_DATASYNC"
      },
      {
        "name": "ohos.permission.DISTRIBUTED_DEVICE_STATE_CHANGE"
      },
      {
        "name": "ohos.permission.GET_DISTRIBUTED_DEVICE_INFO"
      },
      {
        "name": "ohos.permission.GRT_BUNDLE_INFO"
      }
    ]
  }
}